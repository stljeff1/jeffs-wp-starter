<footer id="footer">
	<div class="grid-container">
		<div class="grid-x">
			<div class="cell">
				<ul class="list-unstyled text-left footer-menu">
					{% for item in footer_menu.get_items %}
					  	<li class="{{ item.classes | join(' ') }} pr-3 d-inline-block">
					  		<a href="{{ item.link }}" class="text-white">{{ item.title }}</a>
					  	</li>
					{% endfor %}
				</ul>
			</div>
		</div>
	</div>
</footer>